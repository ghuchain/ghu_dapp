{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "react/prop-types": 0,
      "react/jsx-filename-extension": [
        1, { "extensions": [".js", ".jsx"] }
      ],
      "no-multiple-empty-lines": [
        "error",
        { "max": 1, "maxEOF": 0 }
      ]
    }
}